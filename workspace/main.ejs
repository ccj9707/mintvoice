<body>
  <!-- Modal -->
  <div id="feels" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">원하는 느낌을 선택해주세요.</h4>
        </div>
        <div class="modal-body">
          <div class="container-fluid">
            <form>
              <% tags.feels.forEach(function (tag, index) { %>
                <% if (index % 10 == 0) { %>
                  <div class="row">
                    <% } %>

                      <% if (tag.name.length > 2) { %>
                        <div class="checkbox-inline col-md-1-5">
                          <% } else { %>
                            <div class="checkbox-inline col-md-1">
                              <% } %>

                                <input type="checkbox" value="<%= tag.key %>">
                                <%= tag.name %>
                            </div>
                            <% if (index % 10 == 9 || tags.feels.length-1 == index) { %>
                        </div>
                        <br />
                        <% } %>
                          <% }) %>
            </form>
            </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-info" data-dismiss="modal">선택 완료!</button>
          </div>
        </div>
      </div>
    </div>

